    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Importer",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/import/index.js",
            "envFile": "${workspaceFolder}/.env",
            "args": ["--data=${env:DEBUG_IMPORT_DATA_PATH}"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Spotify find tracks",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/spotify/index.js",
            "envFile": "${workspaceFolder}/.env",
            "args": ["--from=2019-01-05", "--to=2019-01-31"]
        }
    ]
}